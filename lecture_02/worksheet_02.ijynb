    "class Board:\n",
    "\n",
    "    def __init__(self, n):\n",
    "\n",
    "        self.n = n\n",
    "\n",
    "        self.board = [[\"_\" for _ in range(n)] for _ in range(n)]\n",
    "\n",
    "    def __repr__(self):\n",
    "\n",
    "        res = \"\"\n",
    "\n",
    "        for row in self.board:\n",
    "            for col in row:\n",
    "                res += col\n",
    "                res += \" \"\n",
    "            res += \"\\n\"\n",
    "            \n",
    "        return res\n",
    "\n",
    "    def is_legal(self, row, col):\n",
    "\n",
    "        if col >= self.n or row >= self.n:\n",
    "            return False\n",
    "\n",
    "        flag = ('Q' not in [self.board[i][col] for i in range(row)]) & ('Q' not in [self.board[row][i] for i in range(col)])\n",
    "\n",
    "\n",
    "        flag = flag & ('Q' not in [self.board[row-i][col-i] for i in range(1, min(row+1, col+1))])\n",
    "        flag = flag & ('Q' not in [self.board[row-i][col+i] for i in range(1, min(row+1, self.n-col))])\n",
    "        \n",
    "        return flag\n",
    "\n",
    "    def set_queen_at(self, row, col):\n",
    "        self.board[row][col] = \"Q\"\n",
    "\n",
    "    def unset_queen_on(self, row):\n",
    "        self.board[row] = ['_' for _ in range(self.n)]\n",
    "\n",
    "    def get_queen_on(self, row):\n",
    "        for col in range(self.n):\n",
    "            if self.board[row][col] == 'Q':\n",
    "                return col\n",
    "\n",
    "        if row == -1:\n",
    "            raise ValueError(f\"Solutions all found\\n \")\n",
    "        else:\n",
    "            raise ValueError(f\"Programming error at row = {row}\")\n",
    "\n",
    "    def search(self):\n",
    "\n",
    "        row = 0\n",
    "        col = 0\n",
    "\n",
    "        cnt = 0\n",
    "\n",
    "        while row >= 0:\n",
    "\n",
    "            if row < self.n:\n",
    "    \n",
    "                if self.is_legal(row, col):\n",
    "                    self.set_queen_at(row, col)\n",
    "                    row += 1\n",
    "                    col = 0\n",
    "                else:\n",
    "                    col += 1\n",
    "\n",
    "                    while col >= self.n:\n",
    "                            \n",
    "                        col = self.get_queen_on(row - 1)\n",
    "                        self.unset_queen_on(row - 1)\n",
    "                        col += 1\n",
    "                        row -= 1 \n",
    "\n",
    "            else:\n",
    "\n",
    "                cnt += 1\n",
    "\n",
    "                print(\"found a solution\\n\")\n",
    "                print(f\"solution number : {cnt}\\n\")\n",
    "\n",
    "                print(self)\n",
    "                    \n",
    "                col = self.get_queen_on(row - 1)\n",
    "                self.unset_queen_on(row - 1)\n",
    "                col += 1\n",
    "                row -= 1 \n",
    "\n",
    "      \n",
    "                    \n",
    "myBoard = Board(8)\n",
    "myBoard.search()"
